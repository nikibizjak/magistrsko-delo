nil = CON(Nil)

zipWith = FUN(f xs ys ->
    case xs of
    | Nil -> nil
    | Cons xHead xRest ->
        case ys of
        | Nil -> nil
        | Cons yHead yRest ->
            let
                combinedHead = THUNK(f xHead yHead)
                rest = THUNK(zipWith xRest yRest)
                result = CON(Cons combinedHead rest)
            in
                result
)